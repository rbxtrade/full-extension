{
    "manifest_version": 3,
    "name": "Rbxtrade",
    "version": "0.1.0",
    "description": "My Chrome Extension",
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "host_permissions": [
        "*://*.roblox.com/*",
        "*://www.rolimons.com/catalog"
    ]
}